{
  "name": "Azure Improv",
  "version": "1.0",
  "description": "Improving Azure's UI/UX",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://dev.azure.com/*"],
      "run_at": "document_start",
      "js": ["contentscript.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
